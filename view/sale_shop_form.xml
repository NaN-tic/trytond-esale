<data>
    <xpath expr="/form/notebook/page[@id='general']" position="after">
      <page string="eSale" col="4" id="esale">
        <notebook>
            <page string="General" id="general">
              <label name="esale_available"/>
              <field name="esale_available"/>
              <label name="esale_tax_include"/>
              <field name="esale_tax_include"/>
              <label name="esale_price"/>
              <field name="esale_price"/>
            </page>
            <page string="Warehouses" id="warehouses">
              <field name="warehouses" colspan="4"/>
            </page>
            <page string="Countries" id="contries">
              <label name="esale_country"/>
              <field name="esale_country"/>
              <separator name="esale_countrys" colspan="4"/>
              <field name="esale_countrys" colspan="4"/>
            </page>
            <page string="Langs" id="langs" col="2">
              <label name="esale_lang"/>
              <field name="esale_lang"/>
              <separator name="esale_langs" colspan="2"/>
              <field name="esale_langs" colspan="2"/>
            </page>
            <page string="Carriers" id="carriers" col="2">
              <field name="esale_carriers"/>
            </page>
            <page string="Payments" id="payments" col="2">
              <field name="esale_payments"/>
            </page>
          </notebook>
      </page>
    </xpath>
</data>
